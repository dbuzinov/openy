{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}
<form{{ attributes }}>
    <div class="col-xs-1 col-sm-1 col-sm-push-1">
        <a href="#register-or-report" class="icon-back slide-controller"></a>
    </div>
    <div class="col-xs-10 col-xs-push-1 col-sm-4 col-sm-push-1">
        {{ form.mail }}
    </div>
    <div class="col-xs-10 col-xs-push-1 col-sm-3 col-sm-push-1">
        {{ form.membership_id }}
    </div>
    <div class="col-xs-10 col-xs-push-1 col-sm-2 col-sm-push-1">
        {{ form.submit }}
    </div>
    {{ form|without('mail', 'membership_id', 'submit') }}
</form>
<div class="ysr-form-messages ysr-register-form-messages col-xs-10 col-xs-push-2 col-sm-7 col-sm-push-2"></div>
