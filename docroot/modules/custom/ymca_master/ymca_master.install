/**
 * Migrate FileEntity to Media.
 */
function ymca_master_update_8277() {
  $query = \Drupal::entityQuery('file');
  $fileIds = $query->execute();
  $entityTypeManager = \Drupal::entityTypeManager();
  /** @var \Drupal\media_entity\MediaStorage $mediaStorage */
  $mediaStorage = $entityTypeManager->getStorage('media');
  /** @var \Drupal\file\FileStorage $fileStorage */
  $fileStorage = $entityTypeManager->getStorage('file');
  foreach ($fileIds as $fileId) {
    $file = $fileStorage->load($fileId);
    if (!$file) {
      continue;
    }
    $mediaEntity = NULL;
    switch ($file->bundle()) {
      case 'image':
        $id = $file->id();
        $m = $mediaStorage->loadByProperties(
          ['field_media_image' => ['target_id' => $id]]
        );
        if ($m) {
          // Skip media creation, because it is already exists.
          continue;
        }
        // Create new Media entity.
        $mediaEntity = $mediaStorage->create(
          [
            'bundle' => 'image',
            'name' => $file->filename->value,
            'status' => TRUE,
            'field_media_image' => [
              'target_id' => $file->id(),
            ],
            'field_media_caption' => $file->filename->value,
            'field_media_in_library' => TRUE,
            'field_media_mime' => $file->getMimeType(),
          ]
        );
        break;
      case 'document':
      case 'archive':
        $id = $file->id();
        $m = $mediaStorage->loadByProperties(
          ['field_media_document' => ['target_id' => $id]]
        );
        if ($m) {
          // Skip media creation, because it is already exists.
          continue;
        }
        // Create new Media entity.
        $mediaEntity = $mediaStorage->create(
          [
            'bundle' => 'document',
            'name' => $file->filename->value,

            'status' => TRUE,
            'field_media_document' => [
              'target_id' => $file->id(),
            ],
            'field_media_in_library' => TRUE,
            'field_media_mime' => $file->getMimeType(),
            'field_media_size' => $file->getSize(),
          ]
        );
        break;
      case 'audio':
        continue;
      case 'video':
        continue;
    }
    if ($mediaEntity) {
      $status = $mediaEntity->save();
      if ($status) {
        \Drupal::logger('file_media')->debug(
          $mediaEntity->label() . ' created.'
        );
      }
      else {
        throw new \Exception('Failed to save mediaEntity');
      }
    }
  }
}

