/**
 * Migrate deprecated date block.
 */
function ymca_master_update_8269() {
  $blockStorage = \Drupal::entityTypeManager()->getStorage('block_content');
  $oldDateBlocks = $blockStorage->loadByProperties(['type' => 'date_block']);
  $blockStorage->delete($oldDateBlocks);

  $db = \Drupal::database();

  /** @var BlockContent $oldDateBlock */
  foreach ($oldDateBlocks as $oldDateBlock) {
    // Create new block.
    $newDateBlock = $blockStorage->create([
      'type' => 'ygtc_date_block',
      'info' => $oldDateBlock->info->value,
    ]);

    $newDateBlock->set('field_ygtc_end_date', $oldDateBlock->field_end_date->value);
    $newDateBlock->set('field_ygtc_start_date', $oldDateBlock->field_start_date->value);

    $newDateBlock->set(
      'field_ygtc_content_date_end',
      [
        'value' => $oldDateBlock->field_content_date_end->value,
        'format' => $oldDateBlock->field_content_date_end->format,
      ]
    );

    $newDateBlock->set(
      'field_ygtc_content_date_before',
      [
        'value' => $oldDateBlock->field_content_date_before->value,
        'format' => $oldDateBlock->field_content_date_before->format,
      ]
    );

    $newDateBlock->set(
      'field_ygtc_content_date_between',
      [
        'value' => $oldDateBlock->field_content_date_between->value,
        'format' => $oldDateBlock->field_content_date_between->format,
      ]
    );

    $newDateBlock->save();

    $content_tables = [
      'block_content',
      'block_content_revision',
      'block_content_field_data',
      'block_content_field_revision'
    ];

    foreach ($content_tables as $table) {
      // Set the old ID and Revision ID on the new block in the main table.
      $query = $db->update($table);
      $query->condition('id', $newDateBlock->id());

      $fields = [
        'id' => $oldDateBlock->id(),
        'revision_id' => $oldDateBlock->getLoadedRevisionId(),
      ];

      if ($table == 'block_content') {
        $fields['uuid'] = $oldDateBlock->uuid();
      }

      $query->fields($fields);
      $query->execute();
    }

    // Set the old ID and Revision ID on the new block in the field tables.
    $field_tables = [
      'block_content__field_ygtc_end_date',
      'block_content__field_ygtc_start_date',
      'block_content__field_ygtc_content_date_before',
      'block_content__field_ygtc_content_date_between',
      'block_content__field_ygtc_content_date_end',
      'block_content_revision__field_ygtc_end_date',
      'block_content_revision__field_ygtc_start_date',
      'block_content_revision__field_ygtc_content_date_before',
      'block_content_revision__field_ygtc_content_date_between',
      'block_content_revision__field_ygtc_content_date_end',
    ];
    foreach ($field_tables as $table) {
      if ($db->schema()->tableExists($table)) {
        $query = $db->update($table);
        $query->fields([
          'entity_id' => $oldDateBlock->id(),
          'revision_id' => $oldDateBlock->getLoadedRevisionId(),
        ]);
        $query->condition('entity_id', $newDateBlock->id());
        $query->execute();
      }
    }
  }
}

