/**
 * Create Camp du Nord page.
 */
function ymca_master_update_8227() {
  $node = Node::create([
    'type' => 'landing_page',
    'title' => 'Camp du Nord',
    'langcode' => 'en',
    'uid' => '1',
    'status' => 1,
    'field_lp_layout' => 'one_column_clean',
  ]);
  $node->save();
  $query = \Drupal::entityQuery('node')
    ->condition('type', 'landing_page')
    ->condition('title', 'Camp du Nord');
  $page_id = $query->execute();
  $page_id = reset($page_id);
  if (!empty($page_id)) {
    $page = \Drupal::entityTypeManager()
      ->getStorage('node')
      ->load($page_id);
    $paragraph = Paragraph::create([
      'type' => 'camp_du_nord_form',
      'parent_id' => $page->id(),
      'parent_type' => 'node',
      'parent_field_name' => 'field_header_content',
      'status' => 1,
      'uid' => 1,
    ]);
    $paragraph->save();
    $query = \Drupal::entityQuery('paragraph')
      ->condition('type', 'camp_du_nord_form')
      ->condition('parent_id', $page_id);
    $paragraph_id = $query->execute();
    $paragraph_id = reset($paragraph_id);
    $paragraph = \Drupal::entityTypeManager()
      ->getStorage('paragraph')
      ->load($paragraph_id);
    $page->field_header_content->entity = $paragraph;
    $page->path->pathauto = 0;
    $page->save();
    $path = \Drupal::service('path.alias_storage')->save('/node/' . $page_id, '/cdn/search/form', 'en');
  }
  Role::load(RoleInterface::ANONYMOUS_ID)
    ->grantPermission('view published camp du nord personify product entities')
    ->save();
}

