 */
function ymca_master_update_8224() {
  $ids = \Drupal::entityQuery('mapping')
    ->condition('type', 'personify_product')
    ->execute();

  $mapping = Mapping::loadMultiple($ids);
  foreach ($mapping as $map) {
    $package = $map->field_package->value;
    $length = $map->field_session_length->value;
    if ($length == 30) {
      if ($package == 1) {
        $map->set('field_special_member_price', 32);
        $map->set('field_special_non_member_price', 54);
      }
      if ($package == 4) {
        $map->set('field_special_member_price', 128);
        $map->set('field_special_non_member_price', 208);
      }
      if ($package == 20) {
        $map->set('field_special_member_price', 640);
        $map->set('field_special_non_member_price', 1040);
      }
    }
    if ($length == 60) {
      if ($package == 1) {
        $map->set('field_special_member_price', 55);
        $map->set('field_special_non_member_price', 75);
      }
      if ($package == 4) {
        $map->set('field_special_member_price', 220);
        $map->set('field_special_non_member_price', 300);
      }
      if ($package == 20) {
        $map->set('field_member_price', 1099);
        $map->set('field_nonmember_price', 1499);
      }
    }
    $map->set('field_price_available_from', '2017-10-01');
    $map->set('field_price_available_to', '2017-10-16');
    $map->save();
  }
}

