/**
 * Run landing pages migration batch.
 */
function ymca_master_update_8260() {
  $sandbox = [];
  $query = \Drupal::entityQuery('node')
    ->condition('type', 'article')
    ->condition('status', TRUE)
    ->condition('field_related', 'value', 'IS NULL');
  $result = $query->execute();

  // Initialize some variables during the first pass through.
  if (!isset($sandbox['max'])) {
    $sandbox['max'] = count($result);
    $sandbox['progress'] = 0;
  }

  $nodes_per_batch = 50;

  $nids = array_splice($result, 0, $nodes_per_batch);
  $nodes = \Drupal::entityTypeManager()->getStorage('node')
    ->loadMultiple($nids);
  foreach ($result as $nid) {
    $node = \Drupal::entityTypeManager()->getStorage('node')
      ->load($nid);
    \Drupal::service('ymca_migrate_landing_page_importer')->migrate($node);
  }
  $sandbox['progress'] += $nodes_per_batch;
  $sandbox['results']['migrated'][] = implode(', ', $nids);

  $sandbox['message'] = \Drupal::translation()
    ->translate('Migrating nodes: @progress out of @total', [
      '@progress' => $sandbox['progress'],
      '@total' => $sandbox['max'],
    ]);

  if ($sandbox['progress'] < $sandbox['max']) {
    $sandbox['finished'] = $sandbox['progress'] / $sandbox['max'];
  }
  else {
    $sandbox['finished'] = 1;
  }
}

