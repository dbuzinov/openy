  <article class="what-does-it-cost content_wrapper full-width-background-container">
    <div class="full-width-background-image">
      <img src="{{ assets.how_much_bg }}" class="img-responsive" alt="" title="" border="0" />
    </div>
    <article class="container">
      <div class="row">
        {{ block_cost }}
      </div>

      <article class="row content_group row-description">
        <section id="group_0_391450" class="col-sm-12">
          <article class="richtext">
            <p style="color:#5e5e5e;font-size:16px;text-align:center;margin:25px 0 0;">
              *Student memberships are paid on a quarterly basis at $150 per quarter.
            </p>
          </article>
        </section>
      </article>
    </article>
  </article>

  <!-- Ready To Get Started -->
  <section class="ready-to-get-started content_wrapper full-width-background-container"><div class="full-width-background-image">
      <img src="{{ assets.bottom_bg }}" class="img-responsive" alt="" title="" border="0" /><h2>READY TO GET STARTED?</h2>
    </div>
    {{ block_ready_started }}
