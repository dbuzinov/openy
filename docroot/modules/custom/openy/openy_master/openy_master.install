<?php

/**
 * @file
 * Module for hook_update_N.
 */

/**
 * Initial hook_updates runner.
 */
function openy_master_install() {
  for ($i = 8000; $i < 9000; $i++) {
    $function = 'openy_master_update_' . $i;
    if (function_exists($function)) {
      \Drupal::logger('openy_master_install')->info('running: ' . $function);
      $function();
    }
  }
}
