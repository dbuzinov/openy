{#
/**
 * @file
 * Camp du Nord results wrapper template.
 *
 * @ingroup themeable
 */
#}
<div class="cdn-village-teasers">
  <div class="container">
    {% for key, teaser in teasers %}
      <div class="cdn-village-teaser" data-index="{{ key }}">
      {{ teaser.teaser }}
      </div>
    {% endfor %}
  </div>
</div>
<div class="cdn-village-calendars">
  <div class="container">
  {% for key, teaser in teasers %}
    <div class="cdn-calendar" data-index="{{ key }}">
      <div class="description">
        <a href="#" class="close-cal">x</a>
        <a href="#" class="view"><i class="fa fa-eye" aria-hidden="true"></i></a>
        <h2 class="title">{{ teaser.teaser['#title'] }}</h2>
      </div>
      <div class="cdn-calendar-list">
        {{ teaser.calendar.list }}
      </div>
      {{ teaser.calendar.calendar }}
    </div>
  {% endfor %}
  </div>
</div>