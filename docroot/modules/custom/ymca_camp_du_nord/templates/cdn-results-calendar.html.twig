{#
/**
 * @file
 * Camp du Nord List calendar template.
 *
 * @ingroup themeable
 */
#}
<div class="cdn-calendar-list-row{% if data.is_booked %} booked{% endif %}" data-id="{{ data.id }}" data-pid="{{ data.pid }}">
  <a href="#" class="{% if data.is_selected %}selected{% endif %}">
    <div class="date">
      <div class="month">{{ data.date1 }}</div>
      <div class="day">{{ data.date2 }} <span>{{ data.date3 }}</span></div>
    </div>
    <div class="price">${{ data.price }}</div>
  </a>
</div>
