/**
 * Implements hook_field_formatter_info_alter().
 */
function personify_mindbody_sync_field_formatter_info_alter(array &$info) {
  // Let a new field type re-use an existing formatter.
  $info['timestamp']['field_types'][] = 'integer';
  $info['timestamp']['label'] = t('Timestamp (Default)');
}
