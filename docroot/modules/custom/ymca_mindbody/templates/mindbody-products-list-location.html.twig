{#
/**
 * @file
 * Mindbody products list template.
 *
 * Available variables:
 * - attributes: HTML attributes for the containing element.
 *
 * @ingroup themeable
 */
#}
<div class="panel panel-default content-expander">
  <div class="panel-heading">
    <div class="panel-title">
      <h2><a class="accordion-toggle collapsed" data-parent=".panel-group" data-toggle="collapse" href="#collapse-mindbody-products-list-location">Purchase Personal Training</a></h2>
    </div>
  </div>

  <div class="panel-collapse collapse" id="collapse-mindbody-products-list-location" style="height: 0px;">
    <div class="panel-body">

      <div class="row mindbody-products-list-location">
        <div class="container">
          <div class="col-sm-12 col-md-6">
            <h3>{{ 'Members'|t }}</h3>
            {% for key, product_list in products_to_display_member %}
            <strong>{{ key  }}-minute sessions</strong>
            <ul>
              {% for key, product in product_list %}
              <li><a href="{{ product.url }}" target="_blank">${{ product.price }} for {{ product.package }}</a></li>
              {% endfor %}
            </ul>
            {% endfor %}
          </div>
          <div class="col-sm-12 col-md-6">
            <h3>{{ 'Non-members'|t }}</h3>
            {% for key, product_list in products_to_display_non_member %}
            <strong>{{ key  }}-minute sessions</strong>
            <ul>
              {% for key, product in product_list %}
              <li><a href="{{ product.url }}" target="_blank">${{ product.price }} for {{ product.package }}</a></li>
              {% endfor %}
            </ul>
            {% endfor %}
          </div>
        </div>
      </div>

    </div>
  </div>
</div>
