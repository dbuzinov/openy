<?php

/**
 * @file
 * Update hooks for module.
 */

/**
 * Implements hook_uninstall().
 */
function openy_prgf_news_listing_uninstall() {
  $modules_manager = \Drupal::service('openy.modules_manager');
  $modules_manager->removeEntityBundle('paragraph', 'paragraphs_type', 'news_posts_listing');
  $modules_manager->removeModuleConfigs('openy_prgf_news_listing');
}
