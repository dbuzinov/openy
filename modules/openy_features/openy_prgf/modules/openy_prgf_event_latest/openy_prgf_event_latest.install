<?php

/**
 * @file
 * Module install file.
 */

/**
 * Implements hook_uninstall().
 */
function openy_prgf_event_latest_uninstall() {
  $modules_manager = \Drupal::service('openy.modules_manager');
  $modules_manager->removeEntityBundle('paragraph', 'paragraphs_type', 'upcoming_events');
  $modules_manager->removeModuleConfigs('openy_prgf_event_latest');
}
