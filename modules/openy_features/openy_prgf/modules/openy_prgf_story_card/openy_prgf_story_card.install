<?php

/**
 * @file
 * OpenY Paragraph Story Card install file.
 */

/**
 * Implements hook_uninstall().
 */
function openy_prgf_story_card_uninstall() {
  // Remove Story Card content and paragraph type.
  \Drupal::service('openy.modules_manager')
    ->removeEntityBundle('paragraph', 'paragraphs_type', 'story_card');
}
