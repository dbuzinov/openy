<?php

/**
 * @file
 * Module install file.
 */

/**
 * Implements hook_uninstall().
 */
function openy_prgf_event_listing_uninstall() {
  $modules_manager = \Drupal::service('openy.modules_manager');
  $modules_manager->removeEntityBundle('paragraph', 'paragraphs_type', 'event_posts_listing');
  $modules_manager->removeModuleConfigs('openy_prgf_event_listing');
}
