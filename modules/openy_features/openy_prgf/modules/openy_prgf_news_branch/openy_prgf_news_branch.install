<?php

/**
 * @file
 * Update hooks for module.
 */

/**
 * Implements hook_uninstall().
 */
function openy_prgf_news_branch_uninstall() {
  $modules_manager = \Drupal::service('openy.modules_manager');
  $modules_manager->removeEntityBundle('paragraph', 'paragraphs_type', 'latest_news_posts_branch');
  $modules_manager->removeModuleConfigs('openy_prgf_news_branch');
}
