<?php

/**
 * @file
 * Contains ymcali_faq specific functions.
 */

use Drupal\Core\Entity\Display\EntityViewDisplayInterface;
use Drupal\Core\Entity\EntityInterface;

/**
 * Implements hook_entity_view().
 */
function ymcali_faq_entity_view(array &$build, EntityInterface $entity, EntityViewDisplayInterface $display, $view_mode) {
  if ($entity->getEntityTypeId() == 'paragraph' && $entity->bundle() == 'faq') {
    $build['#attached']['library'][] = 'ymcali_faq/paragraph';
  }
}
