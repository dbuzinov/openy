<div class="info-col date col-md-4">
  <div class="date-start">{{ info.date_start }}</div>
  <div class="date-end">{{ 'Until'|t }} {{ info.date_end }}</div>
  <div class="addtocalendar">
    <var class="atc_event">
      <var class="atc_date_start">{{ atc.atc_date_start }}</var>
      <var class="atc_date_end">{{ atc.atc_date_end }}</var>
      <var class="atc_timezone">{{ atc.atc_timezone }}</var>
      <var class="atc_title">{{ atc.atc_title }}</var>
      <var class="atc_description">{{ atc.atc_description }}</var>
      <var class="atc_location">{{ atc.atc_location }}</var>
      <var class="atc_organizer">{{ atc.atc_organizer }}</var>
    </var>
  </div>
</div>