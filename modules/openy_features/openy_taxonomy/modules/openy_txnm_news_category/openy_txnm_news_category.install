<?php

/**
 * @file
 * Update hooks for module.
 */

/**
 * Implements hook_uninstall().
 */
function openy_txnm_news_category_uninstall() {
  $modules_manager = \Drupal::service('openy.modules_manager');
  $modules_manager->removeEntityBundle('taxonomy_term', 'taxonomy_vocabulary', 'news_category', 'vid');
  $modules_manager->removeModuleConfigs('openy_txnm_news_category');
}
